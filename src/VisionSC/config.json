{
  "data_dir": "data",
  "output_dir": "output",
  "gait_parameters": {
    "save_path": "gait_parameters"
  },
  "pose_estimator": {
    "make_video": true,
    "make_csv": true,
    "tracked_csv_dir": "tracked_csv",
    "tracked_video_dir": "tracked_videos"
  },
  "gait_plotting": {
    "generate_kinematic_plots": true,
    "plot_timeseries": true,
    "plot_distributions": true,
    "plot_left_right_comparison": true,
    "plot_fog_visualization": true,
    "timeseries_params": [
      "stride_duration",
      "step_length",
      "gait_speed",
      "swing",
      "cadence"
    ],
    "distribution_params": [
      "stride_duration",
      "step_length",
      "step_duration",
      "swing"
    ],
    "left_right_params": [
      "step_duration",
      "step_length",
      "swing",
      "stance"
    ]
  },
  "event_detection": {
    "algorithm": "zeni",
    "make_plot": true,
    "plots_dir": "plots"
  },
  "preprocessing": {
    "median_filter_window": 31,
    "normalization": true
  },
  "freezing": {
    "velocity_threshold": 0.05,
    "fi_threshold": 2.0,
    "window_size_sec": 2.0,
    "step_size_sec": 0.5
  },
  "visualize": false,
  "analysis_module": "gait"
}